                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="spacing">12</property>
                        <child>
                          <object class="GtkButton" id="four">
                            <property name="label">4</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"4"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="numeric" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="five">
                            <property name="label">5</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"5"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="numeric" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="six">
                            <property name="label">6</property>
