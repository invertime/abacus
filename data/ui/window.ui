                          <property name="column">0</property>
                          <property name="row">4</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="spacing">12</property>
                        <child>
                          <object class="GtkButton" id="dzero">
                            <property name="label">00</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"00"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="numeric" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="zero">
                            <property name="label">0</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"0"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="numeric" />
                            </style>
