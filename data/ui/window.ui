<?xml version="1.0" encoding="UTF-8" ?>
<interface>
  <requires lib="gtk" version="4.0" />
  <menu id="main_menu">
    <item>
      <attribute name="label" translatable="true">About Abacus</attribute>
      <attribute name="action">win.action-about</attribute>
    </item>
  </menu>
  <template class="AbacusMainWindow" parent="HeApplicationWindow">
    <property name="default-width">284</property>
    <property name="default-height">500</property>
    <property name="resizable">0</property>
    <child>
      <object class="GtkWindowHandle">
        <child>
          <object class="HeOverlayButton" id="eq">
            <property name="icon">equals-symbolic</property>
            <property name="typeb">primary</property>
            <child>
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="HeAppBar">
                    <property name="show-back">0</property>
                    <child>
                      <object class="GtkMenuButton" id="menu">
                        <property name="icon-name">open-menu</property>
                        <property name="menu-model">main_menu</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="result">
                    <property name="margin-top">60</property>
                    <property name="margin-start">18</property>
                    <property name="margin-end">18</property>
                    <property name="xalign">0</property>
                    <property name="sensitive">0</property>
                    <style>
                      <class name="calc-label" />
                      <class name="view-title" />
                      <class name="numeric" />
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="entry">
                    <property name="margin-start">8</property>
                    <property name="margin-end">8</property>
                    <property name="xalign">0</property>
                    <property name="width-chars">10</property>
                    <style>
                      <class name="calc-entry" />
                      <class name="display" />
                      <class name="numeric" />
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkGrid">
                    <property name="row-spacing">12</property>
                    <property name="margin-start">18</property>
                    <property name="margin-end">18</property>
                    <property name="margin-bottom">18</property>
                    <child>
                      <object class="GtkBox">
                        <property name="spacing">12</property>
                        <child>
                          <object class="HeFillButton" id="ac">
                            <property name="label">C</property>
                            <property name="action-name">win.action-clear</property>
                            <style>
                              <class name="calc-button" />
                              <class name="destructive-operator" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="perc">
                            <property name="label">%</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"%"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="operator" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="HeFillButton" id="clr">
                            <property name="icon">edit-clear-symbolic</property>
                            <property name="action-name">win.action-delete</property>
                            <style>
                              <class name="calc-button" />
                              <class name="operator" />
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="div">
                            <property name="label">รท</property>
                            <property name="action-name">win.action-insert</property>
                            <property name="action-target">"/"</property>
                            <style>
                              <class name="calc-button" />
                              <class name="operator" />
                            </style>
                          </object>
                        </child>
                        <layout>
                          <property name="column">0</property>
                          <property name="row">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="spacing">12</property>
